id ICON "venom.ico"
1 VERSIONINFO
FILEVERSION     @VENOM_VERSION_MAJOR@,@VENOM_VERSION_MINOR@,@VENOM_VERSION_PATCH@,@VENOM_VERSION_TWEAK@
PRODUCTVERSION  @VENOM_VERSION_MAJOR@,@VENOM_VERSION_MINOR@,@VENOM_VERSION_PATCH@,@VENOM_VERSION_TWEAK@
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "04091252"
    BEGIN
      VALUE "CompanyName", "none"
      VALUE "FileDescription", "GTK+ GUI for TOX"
      VALUE "FileVersion", "@VENOM_VERSION@"
      VALUE "InternalName", "venom.exe"
      VALUE "LegalCopyright", "Copyright (C) 2013"
      VALUE "OriginalFilename", "venom.exe"
      VALUE "ProductName", "Venom"
      VALUE "ProductVersion", "@VENOM_VERSION@"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x409, 1252
  END
END
